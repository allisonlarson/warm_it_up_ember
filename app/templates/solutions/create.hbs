<h4>Submit Your Solution</h4>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="form-horizontal">
      <div class="form-group">
        {{input value=posse_id placeholder="Posse ID" class="form-control"}}
      </div>
      <div class="form-group">
        {{input value=problem_id placeholder="Problem ID" class="form-control"}}
      </div>
      <div class="form-group">
        {{input value=model.content placeholder="Content" class="form-control"}}
      </div>
      <a type='submit' {{action 'create'}}>Submit</a>
    </div>
  </div>
</div>